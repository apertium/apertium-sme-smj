   <rule comment="cop-auxvIF prfprc" > 
      <pattern>
        <pattern-item n="cop-auxvIF"/>
        <pattern-item n="prfprc"/>
      </pattern>
      <action>
        <call-macro n="switch_cases"><with-param pos="1"/><with-param pos="2"/></call-macro>
        <out>
        <chunk>
            <clip pos="2" part="lemh"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
            <clip pos="2" part="lemq"/>
          </chunk>
        </out>
      <choose><when>
        <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
        <out><b pos="1"/></out>
      </when></choose>
      </action>
    </rule> 



    <rule comment="remove-dem">
