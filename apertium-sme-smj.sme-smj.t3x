        </out>
      <choose><when>
        <test><not><equal><b pos="1"/><lit v=" "/></equal></not></test>
        <out><b pos="1"/></out>
      </when></choose>
      </action>
    </rule>
    
    <rule comment="vaj">
      <pattern>
        <pattern-item n="dem"/>
        <pattern-item n="Adp"/>
