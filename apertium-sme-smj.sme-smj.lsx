    <!-- we don't want "go" → "go go" -->
    <e r="LR" w="0.0">
      <i>go<s n="pcle"/><s n="qst"/><t/><d/></i>
    </e>

    <e r="LR" w="1.0">
      <i><w/><t/></i>
      <p><l><s n="qst"/></l><r></r></p>
      <par n="maybe_syntax_tag"/>
      <i><d/></i>
      <p><l></l><r>go<s n="pcle"/><s n="qst"/><d/></r></p>
    </e>
