           o-tmf="OmegaT TMX"
           adminlang="EN-US"
           srclang="sme"
           datatype="plaintext"/>
   <body>
      <tu id="1">
         <tuv lang="sme">
            <seg>Áilu čuoččui guovdu láhtti, šluvggašii guhkes, čáhppes vuovttaidis nugo heavy-rock lávlut lávejitge.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="2">
         <tuv lang="sme">
            <seg>Son doalai Pariissas oston Eiffel-toartnaža mikrofovdnan ja lávllui ”Ella” čoddat vehkui.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="3">
         <tuv lang="sme">
            <seg>– Ella, gii dovdá du geainnuid, vaikko mun lean vázzán daid, mun lean láhppon...</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="4">
         <tuv lang="sme">
            <seg>Kai Somby ja Intrique-joavkku musihkka lei su mielas máilmmi buoremus musihkka.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="5">
         <tuv lang="sme">
            <seg>Dan son lávii guldalit iđđedis gitta eahkedii, álo go ii lean skuvllas.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="6">
         <tuv lang="sme">
            <seg>Lei skábma.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="7">
         <tuv lang="sme">
            <seg>Beaivečuovga illá nagodii oppa lihkkatge.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="8">
         <tuv lang="sme">
            <seg>Dat gávastalai ja mahkalušai guhká ovdalgo vejii bálkestit moadde rukses suotnjara duoddara muođuide.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="9">
         <tuv lang="sme">
            <seg>Áilu lei eahkedis joavdan áhku báikái Leaibejávrái.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="10">
         <tuv lang="sme">
            <seg>Dalle go soai etniin maŋimuš finaiga, muorain ledje ain riebana málen ruškelasttat.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="11">
         <tuv lang="sme">
            <seg>Soai leigga vuodján máŋga čuohte kilomehtera seavdnjada čađa, seavdnjadis seavdnjadii.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="12">
         <tuv lang="sme">
            <seg>Áillu jurdagat joreštedje čavččas ja dálážis.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="13">
         <tuv lang="sme">
            <seg>Orui amas, ahte son ii diehtán makkár máilbmi dál lei stobu olggobealde.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="14">
         <tuv lang="sme">
            <seg>Jogoson jávri lei jikŋon, man oluson lei muohta.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="15">
         <tuv lang="sme">
            <seg>– Áhkku, jogo jávrri alde sáhttá luistet?</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="16">
         <tuv lang="sme">
            <seg>– Ii leatban jávri vel jikŋon.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="17">
         <tuv lang="sme">
            <seg>Gal don iđđes oainnát, mo gátti lea galbmigoahtán.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="18">
         <tuv lang="sme">
            <seg>Iđit njágai Leaibejávráige ja rávkkai Áillu.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="19">
         <tuv lang="sme">
            <seg>Son morihii, gávastalai guhket ja de njuikii fáhkka go fuobmái gos son lei.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="20">
         <tuv lang="sme">
            <seg>Son lenddii guovlat lássaráigge olggos.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="21">
         <tuv lang="sme">
            <seg>Lei jo čuvggodišgoahtán.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="22">
         <tuv lang="sme">
            <seg>Čakčabárut gerre gáddái.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="23">
         <tuv lang="sme">
            <seg>Galbma štuhča ja šávva bahkkii stohposeinniid čađa beljiide.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="24">
         <tuv lang="sme">
            <seg>Guolggat ceagganedje, go son áiccai jiekŋastáluid dievva jávregátti.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="25">
         <tuv lang="sme">
            <seg>Gáttimus stáluid oaivvis steažžái muohtagahpir.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="26">
         <tuv lang="sme">
            <seg>Muhtimat ledje gahperahttá, go bárut ledje jávistan daid jávrái.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="27">
         <tuv lang="sme">
            <seg>– Eadni, vuolgu olggos!</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="28">
         <tuv lang="sme">
            <seg>– Dál jo? Eango moai sáhte vuolgit easkka dan maŋŋá go letne boradan?</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="29">
         <tuv lang="sme">
            <seg>– Ii go dál!</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="30">
         <tuv lang="sme">
            <seg>– Na viegadastte jávregáttis ja de boađát borrat.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="31">
         <tuv lang="sme">
            <seg>– In mun okto...</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="32">
         <tuv lang="sme">
            <seg>– Manne?</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="33">
         <tuv lang="sme">
            <seg>– Danne...</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="34">
         <tuv lang="sme">
            <seg>– Balatgo?</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="35">
         <tuv lang="sme">
            <seg>– I-in, muhto doppe leat stálut.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="36">
         <tuv lang="sme">
            <seg>– Mat stáluid?</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="37">
         <tuv lang="sme">
            <seg>– Geahča ieš!</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="38">
         <tuv lang="sme">
            <seg>– Eai doppe gal oidno matge stáluid!</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="39">
         <tuv lang="sme">
            <seg>– De leat, die áibbas gáttis.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="40">
         <tuv lang="sme">
            <seg>– Meinnetgo dieid jikŋon geđggiid ja bovnnaid?</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="41">
         <tuv lang="sme">
            <seg>– Ii go albma stáluid.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
      <tu id="42">
         <tuv lang="sme">
            <seg>– Buoremus ahte moai boradetne vuos ja de manne ovttas jávregáddái dutkat daid stáluid.</seg>
         </tuv>
         <tuv lang="smj">
            <seg>MONOLINGUAL</seg>
         </tuv>
      </tu>
   </body>
</tmx>
