        <file name="apertium-sme-smj.sme-smj.t5x"/>
        <file name="sme-smj.t5x.bin"/>
      </program>
      <program name="hfst-proc --weight-classes 1 $1">
        <file name="sme-smj.autogen.hfst"/>
      </program>
    </pipeline>
  </mode>


