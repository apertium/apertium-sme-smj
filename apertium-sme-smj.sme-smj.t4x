    <rule> <!--no SOV with complex coordinated objects -->
      <pattern>
        <pattern-item n="SN_Subj"/>
        <pattern-item n="FMainV"/>
        <pattern-item n="SN_Obj"/>
        <pattern-item n="CC"/>
      </pattern>
