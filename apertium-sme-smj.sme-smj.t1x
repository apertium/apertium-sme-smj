<section-def-macros>
  <def-macro n="convert_focus1" npar="1">
    <let>
      <var n="focus_pcle"/>
      <lit v=""/>
    </let>

    <choose>
      <when>
        <test>
          <equal>
            <clip pos="1" side="sl" part="foc"/>
            <lit-tag v="foc_bat"/>
          </equal>
        </test>
        <let>
          <var n="focus_pcle"/>
          <concat>
            <lit v=" ^gus"/>
            <lit-tag v="pcle"/>
            <lit v="$"/>
          </concat>
        </let>
        <let>
            <clip pos="1" side="tl" part="foc"/>
            <lit v=""/>
        </let>
      </when>
      <when>
        <test>
          <or>
            <equal>
              <clip pos="1" side="sl" part="foc"/>
              <lit-tag v="foc_han"/>
            </equal>
            <equal>
              <clip pos="1" side="sl" part="foc"/>
              <lit-tag v="foc_hal"/>
            </equal>
          </or>
        </test>
        <let>
          <var n="focus_pcle"/>
          <concat>
            <lit v=" ^ham"/>
            <lit-tag v="pcle"/>
            <lit v="$"/>
          </concat>
        </let>
        <let>
            <clip pos="1" side="tl" part="foc"/>
            <lit v=""/>
        </let>
      </when>
      <when>
        <test>
          <or>
            <equal>
              <clip pos="1" side="sl" part="foc"/>
              <lit-tag v="foc_gis"/>
            </equal>
            <equal>
              <clip pos="1" side="sl" part="foc"/>
              <lit-tag v="foc_ges"/>
            </equal>
          </or>
        </test>
        <let>
          <var n="focus_pcle"/>
          <concat>
            <lit v=" ^gis"/>
            <lit-tag v="pcle"/>
            <lit v="$"/>
          </concat>
        </let>
        <let>
            <clip pos="1" side="tl" part="foc"/>
            <lit v=""/>
        </let>
      </when>
      <when>
        <test>
          <equal>
            <clip pos="1" side="sl" part="foc"/>
            <lit-tag v="Foc_nai"/>
          </equal>
        </test>
        <let>
          <var n="focus_pcle"/>
          <concat>
            <lit v=" ^aj"/>
            <lit-tag v="pcle"/>
            <lit v="$"/>
          </concat>
        </let>
        <let>
            <clip pos="1" side="tl" part="foc"/>
            <lit v=""/>
        </let>
      </when>
    </choose>
  </def-macro>

  <def-macro n="remove_semtag1" npar="1">
    <let><clip pos="1" side="tl" part="a_sem"/><lit v=""/></let>
  </def-macro>

  <def-macro n="set_syntag1" npar="1">
    <choose>
      <when>
        <test>
          <not>
              <equal>
                <clip pos="1" side="tl" part="syntag"/>
                <lit v=""/>
              </equal>
          </not>
        </test>
        <let>
          <var n="current_syntag"/>
          <clip pos="1" side="tl" part="syntag"/>
        </let>
      </when>
      <otherwise>
        <let>
          <var n="current_syntag"/>
          <lit-tag v="@X"/>
        </let>
      </otherwise>
    </choose>
    <let><clip pos="1" side="tl" part="syntag"/><lit v=""/></let>
  </def-macro>

  <def-macro n="fix_np_tags1" npar="1">
    <choose><when>
      <test>
        <begins-with><clip pos="1" side="tl" part="a_nom"/><lit-tag v="np"/></begins-with>
      </test>
      <let><clip pos="1" side="tl" part="a_attr"/><lit v=""/></let>
      <let><clip pos="1" side="tl" part="a_sem"/><lit v=""/></let> <!-- semtags are optional in the generator. TODO: Are alternatives ever only disambiguated on semtags? -->
      <choose>
        <when>
          <test><and>
            <not><equal><clip pos="1" side="tl" part="case"/><lit-tag v="ess"/></equal></not>
            <not>
              <or>
                <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="sg"/></equal>
                <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="pl"/></equal>
              </or>
            </not>
          </and></test>
          <let>
            <clip pos="1" side="tl" part="a_nom"/>
            <concat><clip pos="1" side="tl" part="a_nom"/><lit-tag v="sg"/></concat>
          </let>
        </when>
      </choose>
      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="case"/><lit v=""/></equal></test>
          <let>
            <clip pos="1" side="tl" part="a_nom"/>
            <concat><clip pos="1" side="tl" part="a_nom"/><lit-tag v="nom"/></concat>
          </let>
        </when>
      </choose>
    </when></choose>
  </def-macro>

  <def-macro n="magic_number1" npar="1">
    <choose>
      <when>
        <test><not>
          <or>
            <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="sg"/></equal>
            <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="pl"/></equal>
          </or>
        </not></test>
        <let>
          <clip pos="1" side="tl" part="a_nom"/>
          <concat><clip pos="1" side="tl" part="a_nom"/><lit-tag v="nn"/></concat>
        </let>
      </when>
    </choose>
  </def-macro>

  <def-macro n="assign_neg_tense1" npar="1">
    <let><var n="cur_verb_temps"/><clip pos="1" side="tl" part="temps"/></let>
    <choose>
      <when>
        <test><and>
          <equal><clip pos="1" side="tl" part="temps"/><lit v=""/></equal>
          <begins-with><clip pos="1" side="tl" part="mood"/><lit-tag v="neg"/></begins-with>
        </and></test>
        <!-- fallback to pres if we haven't matched the main verb -->
        <let><var n="cur_verb_temps"/><lit-tag v="pres"/></let>
        <let><clip pos="1" side="tl" part="mood"/><lit-tag v="neg.indic"/></let>
      </when>
      <when>
        <test><equal><clip pos="1" side="tl" part="conneg"/><lit-tag v="conneg"/></equal></test>
        <let><var n="cur_verb_temps"/><lit v=""/></let>
      </when>
    </choose>
  </def-macro>


  <def-macro n="assign_case1" npar="1">
    <choose>
      <when>
        <test>
          <or>
            <and>
              <equal>
                <clip pos="1" side="sl" part="case"/>
                <lit-tag v="loc"/>
              </equal>
              <equal>
                <clip pos="1" side="sl" part="syntag"/>
                <lit-tag v="@HAB"/>
              </equal>
            </and>
            <in caseless="yes">
              <var n="main_verb"/>
              <list n="verbs_stative"/>
            </in>
          </or>
        </test>
        <let>
          <clip pos="1" side="tl" part="case"/>
          <lit-tag v="ine"/>
        </let>
      </when>
      <when>
        <test>
          <equal>
            <clip pos="1" side="sl" part="case"/>
            <lit-tag v="loc"/>
          </equal>
        </test>
        <let>
          <clip pos="1" side="tl" part="case"/>
          <lit-tag v="ela"/>
        </let>
      </when>
    </choose>
  </def-macro>

</section-def-macros>


