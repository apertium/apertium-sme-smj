     <cat-item tags="Pr.*"/>
  </def-cat>
  
   <def-cat n="po_mod">	
     <cat-item tags="*.*.*.*.@→P"/>
     <cat-item tags="*.*.*.*.*.@→P"/>
     <cat-item tags="*.*.*.@→P"/>
  </def-cat>
  
     <def-cat n="pr_mod">	
