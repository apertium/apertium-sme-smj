  <rule comment="REGLA: NEGVERB CONNEGVERB">
    <pattern>
      <pattern-item n="negverb"/>
      <pattern-item n="connegverb"/>
    </pattern>
    <action>
      <!-- e.g. itgo; but does focus ever go on mainverb? -->
      <call-macro n="convert_focus1"><with-param pos="1"/></call-macro> 
      <choose>
        <when>
          <test><in><clip pos="2" side="sl" part="syntag"/><list n="mainv"/></in></test>
          <let><var n="main_verb"/><clip pos="2" side="sl" part="lemh"/></let>
          <!-- connegverb could be another aux, in which case we *don't* set it as main_verb -->
        </when>
      </choose>
      <out>
        <chunk name="verb" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SV"/></tag>
            <tag><clip pos="1" side="sl" part="syntag"/></tag>
            <tag><clip pos="1" side="sl" part="mood"/></tag>
            <tag><clip pos="1" side="sl" part="foc"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="lemh"/>
            <clip pos="1" side="tl" part="a_verb"/>
            <clip pos="1" side="tl" part="voice"/>
            <clip pos="1" side="tl" part="mood"/>
            <clip pos="2" side="tl" part="temps"/> <!-- temps moved! -->
            <clip pos="1" side="tl" part="conneg"/>
            <clip pos="1" side="tl" part="pers"/>
            <clip pos="1" side="tl" part="nbr"/>
            <clip pos="1" side="tl" part="foc"/>
            <clip pos="1" side="tl" part="lemq"/>
          </lu>
          <var n="focus_pcle"/>
        </chunk>
        <b pos="1"/>
        <chunk name="verb" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SV"/></tag>
            <tag><clip pos="2" side="sl" part="syntag"/></tag>
            <tag><clip pos="2" side="sl" part="mood"/></tag>
