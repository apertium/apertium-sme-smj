  <def-cat n="num">
     <cat-item tags="Num"/>
     <cat-item tags="Num.*"/>
  </def-cat>
  <def-cat n="num_mod"> <!--modifiers of numerals-->
     <cat-item tags="*.*.*.@Num←"/>
