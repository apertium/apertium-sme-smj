             <concat><clip pos="1" side="tl" part="a_mood"/><clip pos="2" side="tl" part="a_temps"/></concat>
        </let>
    <choose><when>
    <test><not><equal><clip pos="2" side="tl" part="lem"/><lit v="liehket"/></equal></not></test> 
        <let><clip pos="2" side="tl" part="a_temps"/><lit v=""/></let>
        </when>
        </choose>
        
      <out>
        <chunk name="verb" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SV"/></tag>
            <tag><clip pos="1" side="sl" part="syntag"/></tag>
            <tag><clip pos="1" side="sl" part="mood"/></tag>
            <tag><clip pos="1" side="sl" part="foc"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="whole"/>
          </lu>
        </chunk>
        <b pos="1"/>
        <chunk name="verb" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SV"/></tag>
            <tag><clip pos="2" side="sl" part="syntag"/></tag>
